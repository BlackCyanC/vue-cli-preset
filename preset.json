{
  "useTaobaoRegistry": true,
  "packageManager": "yarn",
  "latestVersion": "4.4.1",
  "git": false,
  "useConfigFiles": true,
  "cssPreprocessor": "less",
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-typescript": {
      "classComponent": true,
      "useTsWithBabel": true
    },
    "@vue/cli-plugin-router": {
      "historyMode": false
    },
    "@vue/cli-plugin-vuex": {},
    "@vue/cli-plugin-eslint": {
      "config": "standard",
      "lintOn": ["save", "commit"]
    },
    "@vue/cli-plugin-unit-jest": {}
  },
  "configs": {}
}
